//이름과 전화번호를 입력받고 출력

import java.util.*;

public class HW_1 {
	public static void main(String args[]) {
		Scanner input = new Scanner(System.in);

		System.out.print("이름 :");
		String name = input.nextLine();

		System.out.print("전화번호 :");
		String pn = input.nextLine();

		System.out.println(name);
		System.out.println(pn);
	}
}
