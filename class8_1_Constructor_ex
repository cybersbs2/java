public class Student {
	private String name;
	private int age;
	private double score;
	
	public Student() {
		System.out.println("디폴트 생성자");
	}

	public Student(String name, int age, double score) {
		this.name = name;
		this.age = age;
		this.score = score;
	}

	public void studentInfo() {
		System.out.println("학생 이름 : "+this.name+" 학생 나이 : "+this.age+" 학생 학점 : "+this.score);
	}
}
