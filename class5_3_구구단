import java.util.Scanner;

public class HW5_4 {
	public static void main(String args[]) {
		Scanner sc = new Scanner(System.in);
		int cnt = sc.nextInt();
		for(int i=2;i<=9;i=i+cnt) {
			for(int j=1;j<=9;j++) {
			for(int k=0;k<cnt;k++) {
				if(i+k>9) break;
				System.out.printf("%d*%d=%d\t",i+k,j,(i+k)*j);
			}
				System.out.println();
			}
			
		System.out.println();
		}
				
	}
}
